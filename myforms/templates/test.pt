<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" xmlns:tal="http://xml.zope.org/namespaces/tal">
    <head>
        <title>CM Release Tracking Tool</title>
        <meta http-equiv="Content-Type" content="text/html;charset=UTF-8"/>
        <meta name="keywords" content="python web application tool" />
        <meta name="description" content="pyramid web application" />
        <link rel="shortcut icon" href="${request.static_url('myforms:static/images/favicon.ico')}" />
        <link rel="stylesheet" href="${request.static_url('myforms:static/css/my-forms.css')}" type="text/css" media="screen" charset="utf-8" />
        <link rel="stylesheet" href="${request.static_url('myforms:static/js/flexigrid/css/flexigrid/flexigrid.css')}" type="text/css" media="screen" charset="utf-8" />
        <link rel="stylesheet" href="${request.static_url('myforms:static/js/datepicker/timepicker_plug/css/style.css')}" type="text/css" />
        <link rel="stylesheet" href="${request.static_url('myforms:static/js/datepicker/smothness/jquery_ui_datepicker.css')}" type="text/css" />
        <!--[if lte IE 6]>
        <link rel="stylesheet" href="${request.static_url('myforms:static/ie6.css')}" type="text/css" media="screen" charset="utf-8" />
        <![endif]-->
    </head>
    <body>
        <!-- The dark background -->
        <div class="background-cover">&nbsp;</div>
        <div class="background-cover-top">&nbsp;</div>
        <div id="overlay-box-loading" class="overlay-loading">
            <p>Processing&nbsp;...&nbsp;Please Wait</p>
        </div>
        <div id="wrap">
            <div id="top">
                <div class="top align-center">
                    <h1>CM Release Tracking Tool</h1>
                </div>
            </div>
            <div id="middle">
                <form id="create_form">
                    <fieldset>
                        <legend id="title">Submit Release Ticket</legend>
                        <br />
                        <div>
                            <label for="releaseStatus" id="releaseStatusLabel">Release Status</label>
                            <select class="input select-menu" id="releaseStatus" name="status" disabled="disabled">
                                <option tal:repeat="status statuses" tal:attributes="value status.id;label status.label"></option>
                            </select>
                        </div>
                        <div>
                            <label for="releaseType" id="releaseTypeLabel">Release Type</label>
                            <select class="input select-menu" id="releaseType" name="type">
                                <option tal:repeat="type types" tal:attributes="value type.id;label type.label"></option>
                            </select>
                        </div>
                        <div>
                            <label for="releaseProject" id="releaseProjectLabel">Project</label>
                            <select class="input select-menu" id="releaseProject" name="project" size="10"></select>
                        </div>
                        <div class="date">
                            <label for="releaseDate" id="releaseDateLabel">Release Date</label>
                            <input type="text" class="input select-menu" id="releaseDate" name="releaseDate" size="25" maxlength="100" tal:attributes="value date" readonly="readonly" />&nbsp;&nbsp;&nbsp;
                        </div>
                        <div>
                            <label for="releaseVersion" id="releaseVersionLabel">Version</label>
                            <!--input type="text" id="releaseVersion" class="input" name="version" size="100" maxlength="2000" /-->
                            <select class="input" id="releaseVersion" name="version"></select>
                        </div>
                        <div>
                            <label for="login" id="releaseLoginLabel">Login</label>
                            <input type="text" class="input select-menu" id="releaseLogin" name="login" size="25" maxlength="100" tal:attributes="value id" />
                        </div>
                        <div>
                            <label for="password" id="releasePasswordLabel">Password</label>
                            <input type="password" class="input select-menu" id="releasePassword" name="password" size="25" maxlength="100" />
                        </div>
                        <div>
                            <input type="hidden" id="createdBy" name="createdBy" value="149924" />
                            <br />
                            <input type="button" style="width: 8em;" class="control input submit_button submit_create" id="submit_create" value=" Submit " />
                            <input type="button" style="width: 8em;" class="control input cancel_button cancel_create" id="cancel_create" value=" Cancel " />
                        </div>
                    </fieldset>
                </form>
            </div>
            <div id="bottom">
                <div class="bottom">
                    <div id="status-panel">
                        <div id="app-status">
                            <span>User ID:</span>
                            <span id="status-ready" tal:content="id">&nbsp;</span>
                            <span id="notifications">
                                <img id="loading" src="/static/images/loading_bar.gif" alt="loading" />
                                <span>Processing....Please Wait</span>
                            </span>
                            <span id="message" class="ui-widget"></span>
                        </div>
                        <div id="app-options">
                            <ul>
                                <li class="ignore" id="logout"><a href="/logout">Exit Tool</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div id="footer">
          <div class="footer">&copy; Copyright 2011, Shopzilla, Inc.</div>
        </div>
    </body>
    <script type="text/javascript" src="/static/js/jquery.min.js"></script>
    <script type="text/javascript" src="/static/js/jquery.printf.js"></script>
    <script type="text/javascript" src="/static/js/my-forms.js"></script>
    <script type="text/javascript" src="/static/js/datepicker/jquery_ui_datepicker.js"></script>
    <script type="text/javascript" src="/static/js/datepicker/timepicker_plug/timepicker.js"></script>
    <script type="text/javascript" src="/static/js/date-validation.js"></script>
</html>
